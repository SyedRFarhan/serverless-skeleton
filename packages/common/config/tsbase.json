{
  "compilerOptions": {
     "module": "esnext",
     "target": "esnext",
     "lib": [
        "esnext",
        "dom"
     ],
     "sourceMap": true,
     "baseUrl": ".",
     "jsx": "react",
     "resolveJsonModule": true,
     "allowJs": true,
     "skipLibCheck": true,
     "strict": true,
     "experimentalDecorators": true,
     "allowSyntheticDefaultImports": true,
     "moduleResolution": "node",
     "forceConsistentCasingInFileNames": true,
     "noImplicitReturns": true
  },
  "exclude": [
     "node_modules",
     "build",
     "dist",
     "scripts",
     "src/.umi/*",
     "webpack",
     "jest"
  ]
}